<!--custom-tab-bar/index.wxml-->
<van-tabbar active="{{ active }}" 
        placeholder="true"
        active-color="#800000"
        inactive-color="#000"
        bind:change="onChange">
  <van-tabbar-item wx:for="{{ list }}" wx:key="index" icon="{{ item.icon }}"  >
    {{item.text}}
  </van-tabbar-item>
</van-tabbar>